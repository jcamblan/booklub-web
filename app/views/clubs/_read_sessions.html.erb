<% club.read_sessions.unarchived.each do |session| %>
  <%= render 'read_session_card', session: session %>
<% end %>

<% if club.read_sessions.active.empty? %>
  <div class="mt-5 flex">
    <%= render(ButtonComponent.new) { link_to t('.create'), new_club_read_session_path(club) } %>
  </div>
<% end %>
